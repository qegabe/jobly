{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://our.company.com/jobFilter.schema.json",
  "type": "object",
  "properties": {
    "title": {
      "type": "string"
    },
    "minSalary": {
      "type": "string",
      "pattern": "^[0-9]+$"
    },
    "hasEquity": {
      "type": "string",
      "pattern": "^true$|^false$"
    }
  },
  "additionalProperties": false,
  "required": []
}
